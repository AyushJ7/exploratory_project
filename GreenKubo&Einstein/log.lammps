# Viscosity  ##################################

variable         pxy equal pxy
variable         pxz equal pxz
variable         pyz equal pyz

############################# Green-Kubo ############################

fix              SS all ave/correlate $s $p $d                  v_pxy v_pxz v_pyz type auto ave running file profile.gk.3d
fix              SS all ave/correlate 5 $p $d                  v_pxy v_pxz v_pyz type auto ave running file profile.gk.3d
fix              SS all ave/correlate 5 400 $d                  v_pxy v_pxz v_pyz type auto ave running file profile.gk.3d
fix              SS all ave/correlate 5 400 2000                  v_pxy v_pxz v_pyz type auto ave running file profile.gk.3d

variable	 convert_gk equal ${atm2Pa}*${atm2Pa}*${A2m}*${A2m}*${A2m}*${fs2s}
variable	 convert_gk equal 101325*${atm2Pa}*${A2m}*${A2m}*${A2m}*${fs2s}
variable	 convert_gk equal 101325*101325*${A2m}*${A2m}*${A2m}*${fs2s}
variable	 convert_gk equal 101325*101325*1e-10*${A2m}*${A2m}*${fs2s}
variable	 convert_gk equal 101325*101325*1e-10*1e-10*${A2m}*${fs2s}
variable	 convert_gk equal 101325*101325*1e-10*1e-10*1e-10*${fs2s}
variable	 convert_gk equal 101325*101325*1e-10*1e-10*1e-10*1e-15

variable         scale_gk equal (1.0/(${kB}*$T)*vol*$s*dt)*${convert_gk}
variable         scale_gk equal (1.0/(1.3806504e-23*$T)*vol*$s*dt)*${convert_gk}
variable         scale_gk equal (1.0/(1.3806504e-23*300)*vol*$s*dt)*${convert_gk}
variable         scale_gk equal (1.0/(1.3806504e-23*300)*vol*5*dt)*${convert_gk}
variable         scale_gk equal (1.0/(1.3806504e-23*300)*vol*5*dt)*1.0266755625e-35
variable         vxy equal trap(f_SS[3])*${scale_gk}
variable         vxy equal trap(f_SS[3])*3.70756954487086e-10
variable         vxz equal trap(f_SS[4])*${scale_gk}
variable         vxz equal trap(f_SS[4])*3.70756954487086e-10
variable         vyz equal trap(f_SS[5])*${scale_gk}
variable         vyz equal trap(f_SS[5])*3.70756954487086e-10

############################# Einstein ##############################

fix              avstress all ave/time $s $p $d v_pxy v_pxz v_pyz ave one file profile.einstein.3d
fix              avstress all ave/time 5 $p $d v_pxy v_pxz v_pyz ave one file profile.einstein.3d
fix              avstress all ave/time 5 400 $d v_pxy v_pxz v_pyz ave one file profile.einstein.3d
fix              avstress all ave/time 5 400 2000 v_pxy v_pxz v_pyz ave one file profile.einstein.3d

variable         deltasqxy equal (f_avstress[1]*$d*dt)^2
variable         deltasqxy equal (f_avstress[1]*2000*dt)^2
variable         deltasqxz equal (f_avstress[2]*$d*dt)^2
variable         deltasqxz equal (f_avstress[2]*2000*dt)^2
variable         deltasqyz equal (f_avstress[3]*$d*dt)^2
variable         deltasqyz equal (f_avstress[3]*2000*dt)^2

fix              avdeltasq all ave/time $d 1 $d v_deltasqxy v_deltasqxz v_deltasqyz ave running
fix              avdeltasq all ave/time 2000 1 $d v_deltasqxy v_deltasqxz v_deltasqyz ave running
fix              avdeltasq all ave/time 2000 1 2000 v_deltasqxy v_deltasqxz v_deltasqyz ave running

variable	 convert_ein equal ${atm2Pa}*${atm2Pa}*${A2m}*${A2m}*${A2m}*${fs2s}
variable	 convert_ein equal 101325*${atm2Pa}*${A2m}*${A2m}*${A2m}*${fs2s}
variable	 convert_ein equal 101325*101325*${A2m}*${A2m}*${A2m}*${fs2s}
variable	 convert_ein equal 101325*101325*1e-10*${A2m}*${A2m}*${fs2s}
variable	 convert_ein equal 101325*101325*1e-10*1e-10*${A2m}*${fs2s}
variable	 convert_ein equal 101325*101325*1e-10*1e-10*1e-10*${fs2s}
variable	 convert_ein equal 101325*101325*1e-10*1e-10*1e-10*1e-15

variable         scale equal (vol/(2.0*(${kB}*$T)*dt*$d))*${convert_ein}
variable         scale equal (vol/(2.0*(1.3806504e-23*$T)*dt*$d))*${convert_ein}
variable         scale equal (vol/(2.0*(1.3806504e-23*300)*dt*$d))*${convert_ein}
variable         scale equal (vol/(2.0*(1.3806504e-23*300)*dt*2000))*${convert_ein}
variable         scale equal (vol/(2.0*(1.3806504e-23*300)*dt*2000))*1.0266755625e-35
variable         einxy equal f_avdeltasq[1]*${scale}
variable         einxy equal f_avdeltasq[1]*1.85378477243543e-14
variable         einxz equal f_avdeltasq[2]*${scale}
variable         einxz equal f_avdeltasq[2]*1.85378477243543e-14
variable         einyz equal f_avdeltasq[3]*${scale}
variable         einyz equal f_avdeltasq[3]*1.85378477243543e-14

thermo_style     custom step temp press pxy v_vxy v_vxz v_vyz v_einxy v_einxz v_einyz

